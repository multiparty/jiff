{
  "tests": [ "+", "-", "*", "*bgw", "|", "^", "!", "floor", "/", "%" ],
  "suiteConf": {
    "port": 3001,
    "extensions": [ "bigNumber", "fixedpoint" ],
    "generation": {
      "file": "config/fixedpoint/generation.js",
      "function": "generateArithmeticInputs"
    },
    "computation": {
      "file": "config/fixedpoint/computations.js",
      "function": "compute"
    }
  },

  "testConf": {
    "default": {
      "count": 100,
      "parallel": 25,
      "options": {
        "Zp": "7212610147295474909544523785043492409969382148186765460082500085393519556525921455588705423020751421",
        "party_count": 3,
        "decimal_digits": 49,
        "integer_digits": 50,
        "warn": false,
        "safemod": false
      }
    },
    "*": {
      "count": 4,
      "parallel": 2,
      "options": {
        "Zp": "1021",
        "party_count": 3,
        "decimal_digits": 1,
        "integer_digits": 1
      }
    },
    "*bgw": {
      "count": 4,
      "parallel": 2,
      "options": {
        "Zp": "1021",
        "party_count": 3,
        "decimal_digits": 1,
        "integer_digits": 1
      }
    },
    "/": {
      "count": 3,
      "parallel": 2,
      "options": {
        "Zp": "16381",
        "party_count": 3,
        "integer_digits": 2,
        "decimal_digits": 1,
        "crypto_provider": true
      }
    },
    "%": {
      "count": 2,
      "parallel": 2,
      "options": {
        "Zp": "1021",
        "party_count": 3,
        "integer_digits": 2,
        "decimal_digits": 1,
        "warn": false,
        "crypto_provider": true
      }
    },
    "floor": {
      "count": 6,
      "parallel": 3,
      "options": {
        "Zp": 127,
        "party_count": 3,
        "integer_digits": 1,
        "decimal_digits": 1,
        "warn": false,
        "crypto_provider": true
      }
    }
  }
}
